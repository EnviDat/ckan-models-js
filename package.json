{
  "name": "@envidat/ckan-models",
  "version": "0.2.0",
  "description": "JS models for CKAN-based database.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "db:introspect": "dotenv -- prisma introspect",
    "db:generate": "dotenv -- prisma generate",
    "build": "tsc",
    "prepare": "npm run db:generate && npm run build && cp src/index.d.ts dist/index.d.ts",
    "release": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlabext.wsl.ch/EnviDat/ckan-models-js.git"
  },
  "keywords": [
    "envidat",
    "ckan",
    "rdm"
  ],
  "author": "Sam Woodcock",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "dotenv-cli": "^6.0.0",
    "prisma": "^4.6.1",
    "typescript": "^4.9.3"
  },
  "bundleDependencies": [
    "@prisma/client",
    ".prisma"
  ]
}
