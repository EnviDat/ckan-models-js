{
  "name": "@envidat/ckan-models",
  "version": "0.1.5",
  "description": "JS models for CKAN-based database.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "db:introspect": "dotenv -- prisma introspect",
    "db:generate": "dotenv -- prisma generate",
    "build": "tsc",
    "postinstall": "npm exec prisma@4.6.1 generate && npm exec tsc && npm exec cross-os copy-types"
  },
  "cross-os": {
    "copy-types": {
      "linux": "cp src/index.d.ts dist/index.d.ts",
      "darwin": "cp src/index.d.ts dist/index.d.ts",
      "win32": "copy src/index.d.ts dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlabext.wsl.ch/EnviDat/ckan-models-js.git"
  },
  "keywords": [
    "envidat",
    "ckan",
    "rdm"
  ],
  "author": "Sam Woodcock",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "cross-os": "^1.5.0",
    "dotenv-cli": "^6.0.0",
    "prisma": "^4.6.1",
    "typescript": "^4.9.3"
  }
}
